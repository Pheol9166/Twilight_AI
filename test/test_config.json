{
    "embedding_model": "sentence-transformers/all-MiniLM-L6-v2",
    "llm_model": "google/gemma-2-2b-it",
    "llm_params": {
        "provider": "nebius",
        "temperature": 0.3,
        "max_tokens": 512
    },
    "splitter": {
        "CHUNK_SIZE": 1000,
        "CHUNK_OVERLAP": 200
    },
    "retriever": {
        "search_type": "similarity",
        "search_kwargs": {
            "k": 5
        }
    }
    
}